          <systemFunctionsId>1</systemFunctionsId>
          <SysM xmlns="urn:com:ericsson:ecim:ComSysM">
            <sysMId>1</sysMId>
            <OamAccessPoint xmlns="urn:com:ericsson:ecim:RcsOamAccessPoint">
              <oamAccessPointId>1</oamAccessPointId>
			  <accessPoint>ManagedElement=1,Transport=1,Router=OAM,InterfaceIPv4=1,AddressIPv4=1</accessPoint>
            </OamAccessPoint>
            
			 			<NtpServer>
			   				<ntpServerId>1</ntpServerId>
			   				<serverAddress>10.76.176.15</serverAddress>
			   				<administrativeState>UNLOCKED</administrativeState>
			 			</NtpServer>
            
          </SysM>

		      <SecM xmlns="urn:com:ericsson:ecim:ComSecM">
			 			<secMId>1</secMId>
			 			<UserManagement>
			   				<userManagementId>1</userManagementId>
			   				<UserIdentity xmlns="urn:com:ericsson:ecim:RcsUser">
	              				<userIdentityId>1</userIdentityId>
	              				<MaintenanceUser>
	                				<maintenanceUserId>1</maintenanceUserId>
	                				<userName>labuser</userName>
	                				<password>
	                 					<cleartext></cleartext>
	                 					<password>Letmein01</password>
	                				</password>
	              				</MaintenanceUser>
							<!--subjectName>CN=Maintuser</subjectName-->
							<MaintenanceUserSecurity>
	                			<maintenanceUserSecurityId>1</maintenanceUserSecurityId> 
	              			</MaintenanceUserSecurity>
	       					</UserIdentity>
			 			</UserManagement>
       		</SecM>

        </SystemFunctions>
      </ManagedElement>
    </config>
  </edit-config>
</rpc>
